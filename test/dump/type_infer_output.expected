package Type_infer


step test_and (a : bool, b : bool) --> (c : bool)
{
  c : bool = a && b
}

step test_add (a : int, b : int) --> (c : int)
{
  c : int = a + b
}

step test_if (a : bool, b : 'a, c : 'a) --> (d : 'a)
{
  d : 'a = if a then b else c
}

step add_tup (a : (int * int)) --> (b : int)
{
  _ : int, d : int = a;
  c : int, _ : int = a;
  b : int = c + d
}

step test_not (a : bool) --> (b : bool)
{
  b : bool = ~a
}

step test_mul (a : int, b : int) --> (c : int)
{
  c : int = a * b
}

step fst (a : ('a * 'b)) --> (b : 'a)
{
  b : 'a, _ : 'b = a
}